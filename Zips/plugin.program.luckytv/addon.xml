<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.program.luckytv" name="LuckyTv Wizard" version="1.0" provider-name="LuckyTv">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">LuckyTv Wizard</summary>
    <description lang="en">LuckyTv Wizard allows you to get access to the latest LuckyTv Builds and APKs.</description>
    <platform>all</platform>
  </extension>
</addon>